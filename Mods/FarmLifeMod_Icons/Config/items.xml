<configs>
  <remove xpath="/items/item[contains(@modTag,'hasicon')]/property[@name='CustomIcon']" />
  <remove xpath="/items/item[contains(@modTag,'hasicon')]/property[@name='CustomIconTint']" />

  <remove xpath="/items/item[contains(@modTag,'alticon')]/property[@name='CustomIcon']" />

  <setattribute xpath="/items/item[contains(@modTag,'alticon')]/property[@name='AltCustomIcon']" name="name">
    CustomIcon
  </setattribute>

</configs>

